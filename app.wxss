/**app.wxss**/

page{
  background-color: #fff;
  font-size: 26rpx;
  color: #333;
  line-height: 1;
}
.flex-items{
  display: flex;
}
.flex-item{
  flex:1;
}
.top-searchbox{
  padding: 21rpx 30rpx;
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  position: relative;
  background-color: #fff;
}
.fake-search{
 height: 68rpx;
 width: 100%;
 background-color: #f5f5f5;
 border-radius: 60px;
 display: flex;
 align-items: center;
 justify-content: center;
}
.search-icon{
  width: 32rpx;
  height: 32rpx;
  display: inline-block;
  background-image: url(data:image/png;base64,);
  background-size: 32rpx 32rpx;
}
.top-searchbox input{
 height: 68rpx;
 width: 100%;
 background-color: #f5f5f5;
 border-radius: 60px;
 padding-left: 74rpx;
 padding-right: 74rpx;
 font-size: 34rpx;
 color: #444;
 box-sizing: border-box;
}
.fake-search .search-txt{
  font-size: 32rpx;
  color: #999;
  margin-left: 15rpx;
}
.real-search .search-icon{
  position: absolute;
  left: 60rpx;
  top: 40rpx;
}
.real-search .icon-x{
  position: absolute;
  right: 60rpx;
  top: 40rpx;
}
.arrow-right{
  position: relative;
  padding-right: 50rpx;
}
.arrow-right::after{
  content: '';
  width: 16rpx;
  height: 100%;
  position: absolute;
  right: 0;
  top: 0;
  background-image: url(data:image/png;base64,);
  background-size: 16rpx 28rpx;
  background-repeat: no-repeat;
  background-position: right center;
}
.icon-x{
  width: 32rpx;
  height: 32rpx;
  display: inline-block;
  background-image: url(data:image/png;base64,);
  background-size: 32rpx 32rpx;
}
.search-bottom-h1{
  color: #999;
  padding: 20rpx 30rpx;
  margin-top: 50rpx;
}
.cells{
  position: relative;
  background-color: #fff;
}
.cells::before{
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 1px;
    border-top: 1px solid #e5e5e5;
    color: #e5e5e5;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}
.cells::after{
      content: " ";
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    height: 1px;
    border-bottom: 1px solid #e5e5e5;
    color: #e5e5e5;
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
    z-index: 2;
}
.cells .signle-cell{
  height: 120rpx;
  line-height: 60rpx;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 34rpx;
  position: relative
}
.cells .signle-cell::after{
    content: " ";
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    height: 1px;
    border-bottom: 1px solid #e5e5e5;
    color: #e5e5e5;
    -webkit-transform-origin: 0 100%;
    transform-origin: 0 100%;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
    z-index: 2;
}
.cells .signle-cell:last-child::after{
  height: 0
}
.signle-cell .cell-left{
  width: 100%;
  flex-shrink: 1;
  margin-right: 50rpx;
}
.signle-cell .cell-right{
  flex-shrink: 0;
  color: #999
}

.block-btn-box{
  padding: 30rpx 60rpx;
}
.block-btn{
  width: 100%;
  background-color: #B9966F;
  border-radius: 80px;
  font-size: 34rpx;
  color: #FFE5C0;
  text-align: center;
  line-height: 88rpx;
  height: 88rpx;
}

.fixed-bottom-bar{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 96rpx;
  line-height: 96rpx;
  background-color: #fff;
  z-index: 99;
  text-align: center;
  display: flex;
}
.fw-btn{
  width: 208rpx;
  font-size: 30rpx;
  color: #FFE5C0;
  flex-shrink: 0
}
.aw-btn{
  width: 100%;
  flex-shrink: 1;
  color: #B9966F;
  font-size: 34rpx;
}
.bg-primary{
  background-color: #B9966F;
}
.bg-second{
  background-color: #C8A987;
}
.bg-third{
  background-color: #FFE5C0;
}

.mask{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.2);
  z-index: 99999;
  display: none
}
.mask.show{
  display: block;
}
.full-btn{
  width: 100%;
  height: 96rpx;
  background-color: #B9966F;
  font-size: 30rpx;
  color: #FFE5C0;
  text-align: center;
  line-height: 96rpx;
}
.show{
  display: block;
}
.hidden{
  display: none
}
.icon-delete{
  display: inline-block;
  width: 26rpx;
  height: 32rpx;
  background-image: url(data:image/png;base64,);
  background-size: 26rpx 32rpx;
}
.icon-edit{
  display: inline-block;
  width: 32rpx;
  height: 32rpx;
  background-image: url(data:image/png;base64,);
  background-size: 32rpx 32rpx;
}
.order-tip-block{
  height: 68rpx;
  line-height: 68rpx;
  padding: 0 30rpx;
  background-color: #FFE5C0;
  color: #B9966F;
}
.order-tip-block image{
  display: inline-block;
  width: 26rpx;
  height: 26rpx;
  margin-right: 10rpx;
  vertical-align: middle;
}
.order-total-block{
  height: 96rpx;
  line-height: 96rpx;
  display: flex;
  justify-content: space-between;
  padding: 0 30rpx;
  color: #B9966F;
  background-color: #FFE5C0;
}
.order-total-block .total{
  font-size: 30rpx;
}
.tab-block{
  height: 104rpx;
  display: flex;
  justify-content: center;
  background-color: #fff;
  position: relative;
}
.tabs-box{
  display: flex;
  height: 58rpx;
  border-radius: 100px;
  overflow: hidden;
  border: 1rpx solid #FFE5C0;
  align-self: center;
}
.tabs-box .tab-item{
  width: 172rpx;
  text-align: center;
  height: 58rpx;
  line-height: 58rpx;
  font-size: 30rpx;
  color: #B9966F;
}
.tabs-box .tab-item.active{
  border-radius: 100px;
  background-color: #B9966F;
  color: #fff
}
.tab-block .search-icon{
 position: absolute;
 top: 37rpx;
 right: 30rpx;
}

/*  去除滚动条  */
.remove-scroll::-webkit-scrollbar{
  color: transparent;
  width:0;
  height:0;
}