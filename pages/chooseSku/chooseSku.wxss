/* pages/itemDetail/itemDetail.wxss */
@import"../../app.wxss";
.cover{
  position: absolute;
  top:0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0, 0, 0.3);
  z-index: 10000
}
.d-swiper{
  width: 100%;
  height: 750rpx;
}
.d-swiper image{
  width: 100%;
  height: 100%
}
.item-mes-box{
  padding: 25rpx 30rpx;
}
.item-title-row{
  display: flex;
}

.item-title-row .item-title{
  width: 100%;
  flex-shrink: 1;
  font-size: 34rpx;
  height: 100rpx;
  line-height: 50rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
  white-space: normal;
}
.item-title-row .collect{
  width: 100rpx;
  height: 100rpx;
}
.item-tags .item-tag{
  display: inline-block;
  height: 40rpx;
  line-height: 40rpx;
  background-color: #FFE5C0;
  color: #B9966F;
  font-size: 30rpx;
  padding: 0 10rpx;
  margin-right: 20rpx;
  margin-top: 20rpx;
  border-radius: 4rpx;
}
.cells .signle-cell{
  font-size: 30rpx;
  height: 96rpx;
}
.fieldset{
  position: relative;
  width: 180rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  margin: 0 auto;
  z-index: 99
}
.fieldset::after{
  content: '';
  position: absolute;
  top: 50%;
  width: 120rpx;
  left:-120rpx;
  height: 1rpx;
  background-color: #eee;
  z-index: 1
}
.fieldset::before{
  content: '';
  position: absolute;
  top: 50%;
  width: 120rpx;
  right:-120rpx;
  height: 1rpx;
  background-color: #eee;
  z-index: 1
}

.detail-img{
  width: 100%;
}


.icon-g{
  display: inline-block;
  width: 22rpx;
  height: 22rpx;
  background-image: url(data:image/png;base64,);
  background-size: 22rpx 22rpx;
  margin-right: 5rpx;
}

.fixed-actions{
  position: fixed;
  right: 0;
  bottom: 120rpx;
  z-index: 99
}
.fixed-actions .act{
  width: 68rpx;
  height: 68rpx;
  border-radius: 100px;
  text-align: center;
  line-height: 68rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
  margin-right: 15rpx;
  font-size: 22rpx;
  color: #B9966F;
  padding: 0; 
   border: 1px solid #eee;
}

