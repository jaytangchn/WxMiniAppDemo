  box-sizing: content-box;
  margin-left: 0;
   border: 1px solid #eee;
} */
.popup-choose{
  background-color: #fff;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  overflow: visible;
}
.popup-choose .hd{
  height: 228rpx;
  position: relative;
  border-bottom: 1px solid #eee;
  overflow: visible;
}
.popup-choose .hd image{
  width: 210rpx;
  height: 210rpx;
  position: absolute;
  left: 30rpx;
  top: -50rpx;
  border-radius: 10rpx;
}
.popup-choose .hd .item-mes{
  height: 220rpx;
  width: 100%;
  float: left;
  padding: 50rpx 30rpx 20rpx 280rpx;
  overflow: hidden;
  box-sizing: border-box;
}
.popup-choose .hd .item-mes-price{
  height: 25px;
  font-size: 38rpx;
  color: #E66363;
  font-weight: bold;
}
.popup-choose .hd .item-mes-title{
  height:25px;
  color: #999;
  display: block;
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.popup-choose .ft-mes{
  height: 134rpx;
  width: 100%;
  padding:  0 30rpx;
  box-sizing: border-box;
  border-top:1px solid #eee;
  display: flex;
  color: #E66363;
  font-size: 28rpx;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.popup-choose .ft-mes-price{
  font-size: 38rpx;
}
.popup-choose .ft-mes-price .small{
  font-size: 26rpx;
}
.popup-choose .bd{
  max-height: 595rpx;
  padding: 0 30rpx;
  overflow: auto; 
  clear: both;
}
.popup-choose .bd .type-title{
  height: 50rpx;
  line-height: 50rpx;
  /* margin-top: 20rpx; */
  font-size: 28rpx;
  color: #777;
}
.popup-choose .bd .type-lists{
  height:70rpx;
}
.popup-choose .bd .type-item{
  font-size: 28rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  min-width: 150rpx;
  display: inline-block;
  border-radius: 4px;
  border: 1px solid #ccc;
  margin-right: 30rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  overflow: hidden;
  text-overflow: ellipsis;
}
.popup-choose .bd .type-item.choose{
  color: #E66363;
  border-color: #E66363;
}
.popup-choose .num-act-row{
  height: 56rpx;
  margin-top: 20rpx;
  display: flex;
  justify-content: space-between;
}
.popup-choose .num-act-row .num-title{
  font-size: 28rpx;
  color: #777;
  line-height: 56rpx;
}
.popup-choose .num-act-row:last-child{
  margin-bottom: 40rpx;
}
.num-act-box{
  overflow: hidden;
  width: 200rpx;
}
.num-act-box .num-act{
  height: 56rpx;
  width: 56rpx;
  box-sizing: border-box;
  border: 1px solid #ccc;
  display: inline-block;
  vertical-align: top;
  background-size: 26rpx 26rpx;
  background-position: center center;
  background-repeat: no-repeat;
}
.num-act-box .num-act.reduce{
  border-radius: 4px 0 0 4px;
  background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABoAAAAaCAYAAACpSkzOAAAAf0lEQVRIS2NkoBNgpJM9DKMWkR3So0E3GnTwEKBvYjhz5gwXAwPDNbIjAIfG////PzU1NbUGSYN9dPHiRe7fv39/obZFDAwMD01MTBTob9GxY8c4WVlZ91DbR4yMjC9NTEyC4D6itgXYzKNvqhv1ETkhMBpH5IQaWM9o0JEddAC/tx0br7QV9wAAAABJRU5ErkJggg==);
}
.num-act-box .num-act.plus{
  border-radius: 0 4px 4px 0;
  background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABoAAAAaCAYAAACpSkzOAAAAe0lEQVRIS2NkoBNgpJM9DCRZZGxs/B/msP///x88d+6cA7EOHbVoNOjgaWU0MVCeGJAzI7GZkBh1Z8+eBUcPPI6Gn0VGRkYHCAUFIyOjPZKaj////79ASA+sPBxN3pQnb0JhDZIfrY+QQ2k01dEn1RGTMnGpISmOKLEIAMpspRuAbpAvAAAAAElFTkSuQmCC);
}
.num-act-box .num-input{
  display: inline-block;
  width: 80rpx;
  height: 56rpx;
  border: 1px solid #ccc;
  border-left: none;
  border-right: none;
  min-height: 56rpx;
  box-sizing: border-box;
  text-align: center;
}

.parameter{
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #fff;
}
.parameter .parameter-h{
  font-size: 30rpx;
  color: #999;
  height: 96rpx;
  text-align: center;
  line-height: 96rpx;
}
.p-row{
  display: flex;
  height: 70rpx;
  line-height: 70rpx;
  border-top: 1rpx solid #eee;
  font-size: 28rpx;
  padding: 0 30rpx;
}
.p-row .left{
  color: #777;
  width: 240rpx;
}
.p-row .right{
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.half-btn {
  width: 50%;
  height: 96rpx;
  background-color: #B9966F;
  font-size: 30rpx;
  color: #FFE5C0;
  text-align: center;
  line-height: 96rpx;
}
.full-btn{
  width: 100%;
  height: 96rpx;
  background-color: #B9966F;
  font-size: 30rpx;
  color: #FFE5C0;
  text-align: center;
  line-height: 96rpx;
}
.merch-discount{
  height:50rpx;
   margin-top:35rpx; 
  font-size:28rpx;
  color: #777777;
}
.discount{
  margin-top: 0;
  position: relative;
}
.discount .num-input{
  border: 1px solid #ccc;
  border-radius: 10rpx;
  position:absolute;
  right:42rpx;
  height:40rpx;
  
}
.discount .unit{
  position:absolute;
  top:12rpx;
  right:15rpx;
  font-size:28rpx;
  height: 28rpx;
  overflow: hidden;
}


