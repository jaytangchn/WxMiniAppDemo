/* pages/category/category.wxss */
.content{
  position: absolute;
  bottom: 96rpx;
  left: 0;
  right: 0;
  top: 112rpx;
  background: #F9F9F9;
}
.nav-lists{
  position: absolute;
  top: 0;
  left: 0;
  max-height: calc(100% - 130rpx);
  width: 216rpx;
  background-color: #F9F9F9;
  overflow: auto;
  border-bottom:1px solid #eee;
}
.nav-lists .nav-item{
  height: 107rpx;
  line-height: 107rpx;
  color: #777;
  font-size: 28rpx;
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.nav-lists .nav-item.active{
  background-color: #fff;
  color:#b9966f;
}
.nav-contents{
  position: absolute;
  top: 0;
  right: 0;
  left: 216rpx;
  height: 100%;
  overflow: auto;
  background: #fff;
}
.nav-content{
  display: none;
}
.nav-content.active{
  display: block;
}
.nav-content .item-box{
  height: 228rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 20rpx;
  border-bottom: 1rpx solid #eee;
}
.nav-content .item-box .item-image{
  flex-shrink: 0;
  width: 128rpx;
  height: 128rpx;
  border-radius: 2px;
  margin-right: 20rpx;
}
.nav-content .item-box .item-mes{
  flex-shrink: 1;
  width: 100%;
  height: 150rpx;
  overflow: hidden;
}
.item-mes .item-mes-title{
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 32rpx;
  height: 40rpx;
  line-height: 40rpx;
}
.item-mes .item-mes-tags{
  overflow: hidden;
  height: 40rpx;
  margin-top: 8rpx;
}
.item-mes .item-mes-tags .item-mes-tag{
  height: 32rpx;
  line-height: 32rpx;
  background-color: #FFE5C0;
  display: inline-block;
  padding: 0 10rpx;
  font-size: 22rpx;
  color: #B9966F;
  border-radius: 4rpx;
  margin-right: 20rpx;
}
.item-mes-actrow{
  height: 48rpx;
  line-height: 48rpx;
  margin-top: 10rpx;
  display: flex;
  justify-content: space-between;
}
.item-mes-actrow .item-mes-price{
  font-size: 38rpx;
  color: #E66363;
}
.item-mes-actrow .item-mes-price .small{
  font-size: 24rpx;
}
.item-mes-actrow .item-mes-price .old-price{
  text-decoration: line-through;
  color: #BBBBBB;
  margin-left: 10rpx;
  font-size: 22rpx;
}
.item-mes-actrow .item-mes-act{
  width: 48rpx;
  height: 48rpx;
  background-image: url(data:image/png;base64,);
  background-size: 48rpx 48rpx;
}

