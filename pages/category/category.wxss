.fieldset{
  position: relative;
  width: 180rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  margin: 0 auto;
  z-index: 99
}
.fieldset::after{
  content: '';
  position: absolute;
  top: 50%;
  width: 120rpx;
  left:-120rpx;
  height: 1rpx;
  background-color: #eee;
  z-index: 1
}
.fieldset::before{
  content: '';
  position: absolute;
  top: 50%;
  width: 120rpx;
  right:-120rpx;
  height: 1rpx;
  background-color: #eee;
  z-index: 1
}

