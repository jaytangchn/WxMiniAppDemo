  .cells{
    position: relative;
    background-color: #fff;
  }
  .cells::before{
      content: " ";
      position: absolute;
      left: 0;
      top: 0;
      right: 0;
      height: 1px;
      border-top: 1px solid #e5e5e5;
      color: #e5e5e5;
      -webkit-transform-origin: 0 0;
      transform-origin: 0 0;
      -webkit-transform: scaleY(0.5);
      transform: scaleY(0.5);
  }
  .cells::after{
        content: " ";
      position: absolute;
      left: 0;
      bottom: 0;
      right: 0;
      height: 1px;
      border-bottom: 1px solid #e5e5e5;
      color: #e5e5e5;
      -webkit-transform-origin: 0 100%;
      transform-origin: 0 100%;
      -webkit-transform: scaleY(0.5);
      transform: scaleY(0.5);
      z-index: 2;
  }
  .cells .signle-cell{
    height: 120rpx;
    line-height: 60rpx;
    padding: 0 30rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 34rpx;
    position: relative
  }
  .cells .signle-cell::after{
      content: " ";
      position: absolute;
      left: 0;
      bottom: 0;
      right: 0;
      height: 1px;
      border-bottom: 1px solid #e5e5e5;
      color: #e5e5e5;
      -webkit-transform-origin: 0 100%;
      transform-origin: 0 100%;
      -webkit-transform: scaleY(0.5);
      transform: scaleY(0.5);
      z-index: 2;
  }
  .cells .signle-cell:last-child::after{
    height: 0
  }
  .signle-cell .cell-left{
    width: 100%;
    flex-shrink: 1;
    margin-right: 50rpx;
  }
  .signle-cell .cell-right{
    flex-shrink: 0;
    color: #999
  }
  
