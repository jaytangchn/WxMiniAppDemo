/* pages/mine/mine.wxss */
.top-hd{
  height: 430rpx;
  position: relative;
}
.top-hd::after{
  content: '';
  width: 100%;
  height: 188rpx;
  position: absolute;
  left: 0;
  top: 0;
  background-image: url(data:image/png;base64,);
  background-size: 100% 188rpx;
  background-position: top center;
  z-index: 2;
}
.top-hd .avatar{
  position: absolute;
  top: 95rpx;
  left: 50%;
  margin-left: -75rpx;
  width: 150rpx;
  height: 150rpx;
  border-radius: 150px;
  border: 8rpx solid #fff;
  z-index: 99;
  overflow: hidden;
}
.top-hd .avatar image{
   width: 150rpx;
  height: 150rpx;
}
.top-hd .user{
  width: 100%;
  text-align: center;
  font-size: 36rpx;
  position: absolute;
  top: 280rpx;
  left: 0;
}
.top-hd .name{
 font-size: 36rpx;
 margin-top: 10rpx;
}
.top-hd .submes{
  color: #999;
  font-size: 26rpx;
  margin-top: 10rpx;
}

.nav-st{
  margin: 0 30rpx;
  padding: 0 70rpx;
  background-color: #FFE5C0;
  border-radius: 30rpx;
  height: 230rpx;
  color: #B9966F;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  justify-content: center;
}
.nav-st::after{
  content: '';
  width: 160rpx;
  height: 133rpx;
  position: absolute;
  left: 70rpx;
  bottom: 0;
  background-image: url(data:image/png;base64,);
  background-size: 160rpx 133rpx;
}
.nav-st .cn{
  font-size: 36rpx;
}